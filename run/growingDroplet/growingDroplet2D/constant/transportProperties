FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (oil air);

oil
{
    transportModel  Newtonian;
    nu              2.71e-4; 
    rho             888;
}

air
{
    transportModel  Newtonian;
    nu              1.53e-5;
    rho             1.19;
}

sigma               0.0329;

surfaceForces
{
    sigma $sigma;
    surfaceTensionForceModel    RDF; // RDF; //fitParaboloid RDF
    curvFromTr                  off;
    accelerationForceModel      gravity;
    deltaFunctionModel          alphaCSF;
}

