FoamFile
{
    version     2.0;
    format	ascii;
    class	volVectorField;
    object	U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions	[0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    right
    {
        type            zeroGradient;
    }
    top
    {
        type            zeroGradient;
    }
    left
    {
        type            zeroGradient;
    }

    phylic
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }
    phobic
    {
        type            fixedValue;
        value           uniform (0 0 0);
    }

    bottomEmptyFaces
    {
        type    empty;
    }

    topEmptyFaces
    {
        type    empty;
    }
}


