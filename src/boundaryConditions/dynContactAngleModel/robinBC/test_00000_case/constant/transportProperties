FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      transportProperties;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

phases (water_glycerol air);

water_glycerol
{
    transportModel  Newtonian;
    nu              2.507e-05;
    rho             1194.9;
}

air
{
    transportModel  Newtonian;
    nu              1.516e-05;
    rho             1.204;
}


sigma               0.062;
surfaceForces
{
    sigma 0.062;
    surfaceTensionForceModel RDF; //fitParaboloid RDF
    accelerationModel gravity;
    deltaFunctionModel alphaCSF;
}


